{
    "manifest_version": 2,
    "name": "Webpage YOLO",
    "description": "The extension go through your webpage and replace words in any text with ones with similar meanings. Powered by ml5.js and Word2Vec model.",
    "version": "1.0",
    "content_scripts": [{
        "matches": [
            "*://*/*"
        ],
        "js": [
            "ml5.min.js",
            "p5.js",
            "p5.dom.js",
            "html2canvas.min.js",
            "content_script.js"
        ],

        "run_at": "document_end"
    }],
    "icons": {
        "128": "icon-128.png",
        "16": "icon-16.png",
        "48": "icon-48.png"
    }
}